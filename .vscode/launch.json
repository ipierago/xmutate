{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug xmutate (CLI)",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/xmutate",
        "args": [
          "run",
          "--mutator", "summarize",
          "--input", "tests/tmp/input.json",
          "--output", "tests/tmp/output.json",
          "--param", "group_by=category"
        ],
        "cwd": "${workspaceFolder}/xmutate",
        "envFile": "${workspaceFolder}/xmutate/.env",
        "terminal": "integrated"
      }
    ]
  }
  